---
const links = [
  { href: '/', label: 'Home' },
  { href: '/about', label: 'About' },
  { href: '/blog', label: 'Blog' },
  { href: '/contact', label: 'Contact' }
];
---
<nav class="p-4 bg-gray-900 text-white flex justify-between">
  <div class="text-lg font-bold">My Astro Site</div>
  <ul class="flex gap-4">
    {links.map(link => (
      <li><a href={link.href} class="hover:underline">{link.label}</a></li>
    ))}
  </ul>
</nav>
